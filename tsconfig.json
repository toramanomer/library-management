{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 22",
	"_version": "22.0.0",
	"compilerOptions": {
		"lib": [
			"es2023"
		],
		"module": "node16",
		"target": "es2022",
		"strict": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node16",
		"rootDir": "./src",
		"outDir": "./dist",
	}
}